<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="default_list_view_fo_b1b57d83-4186-4308-9379-43325f4e786b" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <list>
        <field name="x_studio_sequence" widget="handle"/>
        <field name="x_name"/>
      </list>
    </field>
    <field name="model">x_ideas</field>
    <field name="name">Default list view for x_ideas</field>
    <field name="type">list</field>
  </record>
  <record id="default_form_view_fo_3a72e2d1-c151-4353-bce8-707065dc17e9" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <form>
        <header/>
        <sheet string="Ideas">
          <widget name="web_ribbon" text="Archived" bg_color="text-bg-danger" invisible="x_active"/>
          <field name="x_active" invisible="1"/>
          <div class="oe_title">
            <h1>
              <field name="x_name" required="1" placeholder="Name..."/>
            </h1>
          </div>
          <group name="studio_group_278707">
            <group name="studio_group_278707_left"/>
            <group name="studio_group_278707_right"/>
          </group>
        </sheet>
        <chatter/>
      </form>
    </field>
    <field name="model">x_ideas</field>
    <field name="name">Default form view for x_ideas</field>
    <field name="type">form</field>
  </record>
  <record id="default_search_view__d7c4cf2f-5d71-4336-b657-3c98ea6efa2b" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <search>
        <field name="x_name"/>
        <separator/>
        <filter string="Archived" name="archived_x_ideas" domain="[['x_active', '=', False]]"/>
        <separator/>
      </search>
    </field>
    <field name="model">x_ideas</field>
    <field name="name">Default search view for x_ideas</field>
    <field name="type">search</field>
  </record>
</odoo>
